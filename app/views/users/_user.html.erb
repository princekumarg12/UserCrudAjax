<div id="<%= dom_id user %>" class="card mt-3">
  <div class="card-body">
    <h5 class="card-title"><%= user.name %></h5>

    <p class="card-text">
      <strong>Address:</strong>
      <%= user.address %>
    </p>

    <p class="card-text">
      <strong>Contact:</strong>
      <%= user.contact %>
    </p>
  </div>
</div>
